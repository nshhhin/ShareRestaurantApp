# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'ShareRestaurantApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'GoogleMaps', '~>2.7.0'
    pod 'RxSwift', '~>4.4.0'
    pod 'RxCocoa', '~>4.4.0'
    pod 'Alamofire', '~>4.7.3'
    pod 'AlamofireImage', '~>3.4.1'
    pod 'SwiftyJSON', '~>4.2.0'

  # Pods for ShareRestaurantApp

  target 'ShareRestaurantAppTests' do
    inherit! :search_paths
    pod 'RxBlocking', '~>4.4.0'
    pod 'RxTest', '~>4.4.0'
  end

  target 'ShareRestaurantAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
